package classes;

public class CustomMethods {
	
	public int addition(int x, int y) {
		return x+y;
	}
	
	public int subtraction(int x, int y) {
		return x-y;
	}
	
	public float division(int x, int y) throws ArithmeticException{
		return x/y;
	}
	
	public int multiplication(int x, int y) {
		return x*y;
	}

}
