package wipro.day2.demo;

public class this_demo {
	
	class Student {
		int rollNumber;
		String sName;
		String division;
	
	
		Student(int rollNumber, String sName, String division){
			this.rollNumber = rollNumber;
			this.sName = sName;
			this.division = division;
		}
	
		void display() {
			System.out.println(rollNumber+" "+sName+" "+division);
		}
	
	}

}
